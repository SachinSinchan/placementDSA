import java.util.*;
public class kk{
    public static int fib(int n){
        if(n==0||n==1){
            return n;
        }
        else{
            int fibnm1=fib(n-1);
            int fibnm2=fib(n-2);
            int fn=fib(n-1)+fib(n-2);
            return fn;
        }
    }
    public static void main(String args[]){
        int n=8;
        System.out.println(fib(n));
    }
}
